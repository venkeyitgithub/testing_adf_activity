					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": {
							"value": "SELECT CONCAT('_',\nCAST(YEAR(GETUTCDATE()) as Char(4)), \nCASE WHEN LEN(Month(GETUTCDATE())) < 2 THEN CONCAT('0', Month(GETUTCDATE())) ELSE CAST(Month(GETUTCDATE()) AS CHAR(2)) END, \nCASE WHEN LEN(Day(GETUTCDATE())) < 2 THEN CONCAT('0', Day(GETUTCDATE())) ELSE CAST(Day(GETUTCDATE()) AS CHAR(2)) END, \nCASE WHEN LEN(Datepart(HOUR, GETUTCDATE())) < 2 THEN CONCAT('0', Datepart(HOUR, GETUTCDATE())) ELSE CAST(Datepart(HOUR, GETUTCDATE()) AS CHAR(2)) END, \nCASE WHEN LEN(Datepart(Minute, GETUTCDATE())) < 2 THEN CONCAT('0', Datepart(Minute, GETUTCDATE())) ELSE CAST(Datepart(Minute, GETUTCDATE()) AS CHAR(2)) END,\nCASE WHEN LEN(Datepart(SECOND, GETUTCDATE())) < 2 THEN CONCAT('0', Datepart(SECOND, GETUTCDATE())) ELSE CAST(Datepart(SECOND, GETUTCDATE()) AS CHAR(2)) END, '.json') AS filenametype",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_synapse_control_source",
						"type": "DatasetReference"
					}
				}
			},
