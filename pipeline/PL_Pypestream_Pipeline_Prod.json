			{
				"name": "ACT_LKP_Get_Filetype",
				"description": "Get File Type",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "ACT_LKP_Get_Execution_Run_ID",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
